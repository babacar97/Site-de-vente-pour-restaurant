  `params` text NOT NULL,
  PRIMARY KEY (`id_leoslideshow_slides`)
) ENGINE=ENGINE_TYPE AUTO_INCREMENT=1 DEFAULT CHARSET=utf8;

CREATE TABLE IF NOT EXISTS `PREFIX_leoslideshow_slides_lang` (
